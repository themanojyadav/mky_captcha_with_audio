{
    "name": "mky/captcha-with-audio",
    "description": "A customizable CAPTCHA package with optional audio support for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mky",
            "email": "mky@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0",
        "illuminate/validation": "^12.0",
        "intervention/image": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Mky\\CaptchaWithAudio\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mky\\CaptchaWithAudio\\CaptchaServiceProvider"
            ],
            "aliases": {
                "MkyCaptcha": "Mky\\CaptchaWithAudio\\Facades\\MkyCaptcha"
            }
        }
    },
    "minimum-stability": "stable"
}
